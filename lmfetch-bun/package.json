{
  "name": "lmfetch",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "lmfetch": "./bin/lmfetch.ts"
  },
  "scripts": {
    "dev": "bun run bin/lmfetch.ts",
    "build": "bun build --compile --minify bin/lmfetch.ts --outfile=dist/lmfetch",
    "build:linux-x64": "bun build --compile --minify --target=bun-linux-x64 bin/lmfetch.ts --outfile=dist/lmfetch-linux-x64",
    "build:linux-arm64": "bun build --compile --minify --target=bun-linux-arm64 bin/lmfetch.ts --outfile=dist/lmfetch-linux-arm64",
    "build:darwin-x64": "bun build --compile --minify --target=bun-darwin-x64 bin/lmfetch.ts --outfile=dist/lmfetch-darwin-x64",
    "build:darwin-arm64": "bun build --compile --minify --target=bun-darwin-arm64 bin/lmfetch.ts --outfile=dist/lmfetch-darwin-arm64",
    "build:windows-x64": "bun build --compile --minify --target=bun-windows-x64 bin/lmfetch.ts --outfile=dist/lmfetch-windows-x64.exe",
    "build:all": "bun run build:linux-x64 && bun run build:linux-arm64 && bun run build:darwin-x64 && bun run build:darwin-arm64 && bun run build:windows-x64"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "ai": "^4.0.0",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "ignore": "^6.0.0",
    "js-tiktoken": "^1.0.0",
    "marked": "^17.0.1",
    "marked-terminal": "^7.3.0",
    "ora": "^8.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
